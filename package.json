{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --dotenv env/.env.dev",
    "dev:test": "nuxt dev --dotenv env/.env.prd",
    "dev:prd": "nuxt dev --dotenv env/.env.test",
    "build": "nuxt build --dotenv env/.env.dev",
    "build:test": "nuxt build --dotenv env/.env.prd",
    "build:prd": "nuxt build --dotenv env/.env.test",
    "generate": "nuxt generate --dotenv env/.env.dev",
    "generate:test": "nuxt generate --dotenv env/.env.prd",
    "generate:prd": "nuxt generate --dotenv env/.env.test",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:css": "stylelint **/*.{css,less,scss,vue}",
    "lint:js": "eslint **/*.{js,ts,tsx,vue}",
    "lint:all": "eslint **/*.{js,ts,tsx,vue} & stylelint **/*.{css,less,scss,vue}",
    "lint:fix": "eslint **/*.{js,ts,tsx,vue} --fix & stylelint **/*.{css,less,scss,vue} --fix",
    "type-check": "vue-tsc --noEmit",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.5",
    "@types/dplayer": "^1.25.2",
    "@types/lodash-es": "^4.17.7",
    "@vueuse/nuxt": "^10.2.0",
    "@yikoyu/eslint-config-prettier": "^1.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.6.0",
    "sass": "^1.63.6",
    "typescript": "^5.0.4",
    "vite-plugin-checker": "^0.6.1",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.1"
  },
  "dependencies": {
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.11",
    "@vueuse/core": "^10.2.0",
    "@vueuse/router": "^10.2.1",
    "assjs": "^0.0.11",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "dplayer": "^1.27.1",
    "hls.js": "^1.4.7",
    "lodash-es": "^4.17.21",
    "mpegts.js": "^1.7.3",
    "pinia": "^2.1.4"
  }
}
